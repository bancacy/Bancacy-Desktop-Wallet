{
  "name": "BANCACYDesktop",
  "version": "0.0.12",
  "author": "Info <Info@bancacy.com>",
  "repository": {
    "type": "git",
    "url": "git://github.com/bancacy/Bancacy-Desktop-Wallet"
  },
  "description": "BANCACY Desktop Wallet",
  "license": "MIT",
  "main": "./dist/electron/main.js",
  "scripts": {
    "elec": "./node_modules/.bin/electron-rebuild.cmd",
    "elec:mac": "$(npm bin)/electron-rebuild",
    "build": "node .electron-vue/build.js && electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": "electron-builder install-app-deps && npm run lint:fix",
    "css": "npx tailwind build src/renderer/assets/css/tailwind.scss -o src/renderer/assets/css/tailwind.css"
  },
  "build": {
    "productName": "BANCACY",
    "appId": "org.Bancacy.Desktop-Wallet",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "mac": {
      "icon": "build/icons/icon.png"
    },
    "win": {
      "icon": "build/icons/icon.png",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "linux": {
      "icon": "build/png"
    },
    "publish": {
      "provider": "github"
    },
    "nsis": {
      "shortcutName": "BANCACY",
      "artifactName": "BANCACY.Installable.${version}.${ext}"
    },
    "portable": {
      "artifactName": "BANCACY.Portable.${version}.${ext}"
    },
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ],
      "artifactName": "BANCACY.Mac.Installable.${version}.${ext}"
    }
  },
  "dependencies": {
    "axios": "^0.16.1",
    "bignumber.js": "^9.0.0",
    "clipboard": "^2.0.1",
    "compare-versions": "^3.4.0",
    "eth-lightwallet": "3.0.1",
    "ethjs-signer": "0.1.1",
    "fs-extra-p": "7.0.0",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "qrcode": "^1.2.0",
    "vue": "^2.3.3",
    "vue-electron": "^1.0.6",
    "vue-router": "^2.5.3",
    "web3": "1.0.0-beta.34",
    "websocket": "^1.0.29"
  },
  "devDependencies": {
    "ajv": "^6.9.1",
    "babel-core": "6.26.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babili-webpack-plugin": "0.1.2",
    "cfonts": "^1.1.3",
    "chalk": "^2.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.4",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "3.0.10",
    "electron-builder": "20.44.4",
    "electron-debug": "2.0.0",
    "electron-devtools-installer": "2.2.4",
    "electron-installer-dmg": "2.0.0",
    "electron-packager": "12.2.0",
    "electron-rebuild": "^1.8.6",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^5.5.0",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "style-loader": "^0.18.2",
    "tailwindcss": "^1.0.4",
    "url-loader": "^0.5.9",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^13.0.5",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.5.2",
    "webpack-dev-server": "^2.7.1",
    "webpack-hot-middleware": "^2.18.2"
  }
}
